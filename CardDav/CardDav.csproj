<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Description>Implements RFC 6352</Description>
    <PackageLicenseExpression></PackageLicenseExpression>
    <Copyright>vknht</Copyright>
    <PackageProjectUrl>https://github.com/vknht/CardDav</PackageProjectUrl>
    <RepositoryUrl>git@github.com:vknht/CardDav.git</RepositoryUrl>
    <PackageTags>CardDav RFC6352 RFC 6352 CardDav4</PackageTags>
    <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseFile>License.md</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\License.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
